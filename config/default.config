client_max_body_size 12
error_page 403 www/error_page_403.html
error_page 404 www/error_page_404.html
error_page 500 www/error_page_500.html

server {
	listen 0.0.0.0:10
}

server {
	client_max_body_size 12
	listen 9
	server_name localhost

	cgi_dir www/cgi-bin/

	error_page 403 www/error_page_403.html
	error_page 404 www/error_page_404.html
	error_page 500 www/error_page_500.html

	upload_pass www/uploads


	location /includes {
		return 301 www.new-name.com$request_uri
		client_max_body_size 24
		error_page 404 none
		root incs/

	}

	location /delete {
		root www/uploads/
		disable_methods POST
	}

	location /put {
		root www/uploads/
		disable_methods DELETE
	}

	location /sources/bonsoir {
		autoindex on
		root srcs/
	}

	location /dir {
		index index.html;
		root srcs/
	}

	location /noget {
		disable_methods GET
		root www/
	}

	location /nonothing {
		disable_methods GET  DELETE 
		root www/
	}

	location /admin {
		index index.php
		disable_methods POST POST
		root www/
	}

	location / {
		index index.php
		root www/
	}
}

